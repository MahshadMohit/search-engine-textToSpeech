<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.effect.Reflection?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="506.0" prefWidth="805.0" style="-fx-background-color: #FCFCFC;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.searchengineproj.EveryPage">
    <children>
      <TextArea fx:id="textArea" disable="true" layoutX="112.0" layoutY="83.0" prefHeight="324.0" prefWidth="551.0" scrollLeft="10.0" scrollTop="10.0" style="-fx-background-color: TRANSPARENT; -fx-border-color: #7F7F7F;" visible="false">
         <font>
            <Font name="Arial" size="12.0" />
         </font>
      </TextArea>
        <ImageView fitHeight="396.0" fitWidth="627.0" layoutX="87.0" layoutY="53.0" opacity="0.17" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../bb.jpg" />
            </image>
        </ImageView>
        <ImageView fitHeight="60.0" fitWidth="166.0" layoutX="630.0" layoutY="19.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../searchIcon.png" />
            </image>
        </ImageView>
        <Button fx:id="back" layoutX="18.0" layoutY="27.0" mnemonicParsing="false" onAction="#setBack" style="-fx-background-color: TRANSPARENT;" text="Back" textFill="#fc3838d0">
            <font>
                <Font name="Berlin Sans FB" size="18.0" />
            </font>
            <effect>
                <Reflection bottomOpacity="0.14" fraction="0.36" topOpacity="0.43" />
            </effect>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
      <ImageView fx:id="save" fitHeight="33.0" fitWidth="41.0" layoutX="33.0" layoutY="449.0" onMouseClicked="#setSave" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../download.png" />
         </image>
      </ImageView>
    </children>
</AnchorPane>
